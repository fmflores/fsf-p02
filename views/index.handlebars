<div class="container my-5">
    <div class="row">
        <div class="col">
            {{> recentPodcasts-list }}
        </div>
    </div>
    <div class="row">
        <div class="col">
            {{> updatedSubscriptions-list }}
        </div>
    </div>
    <div class="row mt-5">
        <div class="col-md-6 offset-md-3">
            <div class="input-group mb-3">
                <input type="text" class="form-control" placeholder="Podcast Name" aria-label="Podcast Name" id="podcastName">
                <div class="input-group-append">
                    <button class="btn btn-outline-success" type="button" id="addPodcast">Add</button>
                </div>
            </div>
        </div>
    </div>
</div>